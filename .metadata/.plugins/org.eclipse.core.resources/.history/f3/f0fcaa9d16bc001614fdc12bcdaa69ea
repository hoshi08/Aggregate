
//if文練習

public class ifSentence_1 {
    public static void main(String[] args){
        int age;
        age =40;
        if(age < 20 ) {
     	    System.out.println("未成年ですね");
        } else if(age < 30 ) {
     	    System.out.println("お酒が飲めます");
        } else if(age < 40 ) {
    	    System.out.println("三十路ですね");
        } else  {
    	    System.out.print("アラフォー");
       }
    }
}

